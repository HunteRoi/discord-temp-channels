{
  "name": "@hunteroi/discord-temp-channels",
  "version": "2.0.0",
  "description": "Based on Androz2091's package, this is a simple framework to facilitate the creation of a temporary voice & text channels system using Discord.js",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/HunteRoi/discord-temp-channels/issues"
  },
  "homepage": "https://github.com/HunteRoi/discord-temp-channels#readme",
  "keywords": [
    "discordjs",
    "temporary channels",
    "discordbot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HunteRoi/discord-temp-channels.git"
  },
  "author": "HunteRoi",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" --tab-width 4",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "example": "npm run build & node ./example/index.js",
    "example2": "npm run build & node ./example/index2.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@types/ws": "^7.4.7",
    "discord-sync-commands": "^0.3.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ]
}
